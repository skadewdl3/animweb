{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "echo \"Building wasm code\" && npm run wasm && \"Building nuxt code\" && nuxt build",
    "dev": "concurrently \"npm run watch:wasm\" \"nuxt dev\"",
    "generate": "npm run asbuild && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "node tests",
    "start": "npx serve .",
    "watch:wasm": "nodemon --watch wasm --ext rs --exec \"npm run wasm\"",
    "wasm": "cd wasm && wasm-pack build --target web && cd .."
  },
  "devDependencies": {
    "@types/node": "^18",
    "concurrently": "^8.1.0",
    "nodemon": "^2.0.22",
    "nuxt": "^3.5.0"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@codemirror/lang-javascript": "^6.1.8",
    "@types/animejs": "^3.1.7",
    "@types/p5": "^1.6.2",
    "@types/three": "^0.152.0",
    "@types/uuid": "^9.0.1",
    "abs-svg-path": "^0.1.1",
    "animejs": "^3.2.1",
    "assemblyscript": "^0.27.5",
    "axios": "^1.4.0",
    "calculate-size": "^1.1.1",
    "codemirror": "^6.0.1",
    "firebase": "^9.22.0",
    "firebase-admin": "^11.8.0",
    "isosurface": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "mathjax-full": "^3.2.2",
    "mathjs": "^11.8.0",
    "npm": "^9.6.7",
    "p5": "^1.6.0",
    "polymorph-js": "^1.0.2",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "svg-flatten": "^1.0.1",
    "svg-path-commander": "^2.0.5",
    "three": "^0.152.2",
    "troika-three-text": "^0.47.2",
    "uuid": "^9.0.0",
    "vectorize-text": "^3.2.2",
    "vite-plugin-node-polyfills": "^0.8.2",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm-pack": "^0.1.12"
  }
}
